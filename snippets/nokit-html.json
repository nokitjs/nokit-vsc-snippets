{
    "nokit-view-action-url": {
        "prefix": "nokit-view-action-url",
        "body": [
            "<%= $.route.actionUrl(\"${action}\") %>",
            "$0"
        ],
        "description": "Nokit View Action URL"
    },
    "nokit-view-block": {
        "prefix": "nokit-view-block",
        "body": [
            "<% $.beginBlock(\"${name}\") %>",
            "    $1",
            "<% $.endBlock() %>",
            "$0"
        ],
        "description": "Nokit View Block"
    },
    "nokit-view-code": {
        "prefix": "nokit-view-code",
        "body": [
            "<% $1 %>",
            "$0"
        ],
        "description": "Nokit View Code Block"
    },
    "nokit-view-content": {
        "prefix": "nokit-view-content",
        "body": [
            "<% $.beginContent(\"${name}\") %>",
            "    $1",
            "<% $.endContent() %>",
            "$0"
        ],
        "description": "Nokit View Content"
    },
    "nokit-view-define-block": {
        "prefix": "nokit-view-define-block",
        "body": [
            "<% $.defineBlock(\"${name}\") %>",
            "$0"
        ],
        "description": "Nokit View Define Block"
    },
    "nokit-view-each": {
        "prefix": "nokit-view-each",
        "body": [
            "<% $.each(${list},function(${i},${item}){ %>",
            "    $1",
            "<% }); %>",
            "$0"
        ],
        "description": "Nokit View Each"
    },
    "nokit-view-form": {
        "prefix": "nokit-view-form",
        "body": [
            "<form method=\"${post}\" action=\"${action}\" enctype=\"${application/x-www-form-urlencoded}\">",
            "    $1",
            "</form>",
            "$0"
        ],
        "description": "Nokit View Form"
    },
    "nokit-view-if-else": {
        "prefix": "nokit-view-if-else",
        "body": [
            "<% if(${expr}){ %>",
            "    $1",
            "<% } else { %>",
            "    $2",
            "<% } %>",
            "$0"
        ],
        "description": "Nokit View If-Else"
    },
    "nokit-view-if": {
        "prefix": "nokit-view-if",
        "body": [
            "<% if(${expr}){ %>",
            "    $1",
            "<% } %>",
            "$0"
        ],
        "description": "Nokit View If"
    },
    "nokit-view-include": {
        "prefix": "nokit-view-include",
        "body": [
            "<%= $.include(\"${view}\") %>",
            "$0"
        ],
        "description": "Nokit View Include"
    },
    "nokit-view-master": {
        "prefix": "nokit-view-master",
        "body": [
            "<% $.master(\"${layout}\") %>",
            "$0"
        ],
        "description": "Nokit View Master"
    },
    "nokit-view-placeholder": {
        "prefix": "nokit-view-placeholder",
        "body": [
            "<% $.defineBlock(\"${name}\") %>",
            "$0"
        ],
        "description": "Nokit View PlaceHolder"
    },
    "nokit-view-print": {
        "prefix": "nokit-view-print",
        "body": [
            "<%= ${x} %>",
            "$0"
        ],
        "description": "Nokit View Print"
    }
}