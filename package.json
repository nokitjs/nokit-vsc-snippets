{
  "name": "nokit",
  "displayName": "Nokit",
  "publisher": "houfeng",
  "description": "Nokit support for vscode",
  "version": "0.1.5",
  "engines": {
    "vscode": "0.10.3"
  },
  "categories": [
    "Languages",
    "Snippets"
  ],
  "contributes": {
    "languages": [
      {
        "id": "nsp",
        "aliases": [
          "nsp",
          "NSP"
        ],
        "extensions": [
          ".nsp",
          ".nsp.html",
          ".nsp.htm"
        ]
      }
    ],
    "grammars": [
      {
        "language": "nsp",
        "scopeName": "text.html.js",
        "path": "./syntaxes/nsp.tmLanguage"
      }
    ],
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "html",
        "path": "./snippets/html.json"
      },
      {
        "language": "nsp",
        "path": "./snippets/html.json"
      }
    ]
  },
  "keywords": [
    "nokit",
    "nokitjs",
    "vscode",
    "snippets",
    "snippet"
  ],
  "scripts": {
    "gen": "node ./bin/snippets-generator.js"
  }
}